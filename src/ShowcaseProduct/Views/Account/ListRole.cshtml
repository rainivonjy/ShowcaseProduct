@using ShowcaseProduct.Models.Account
@model ApplicationRole
@{
    ViewBag.Title = "Add Role";
}

<h2>@ViewBag.Title</h2>
<div class="row">

    <div class="col-md-8">
        <div class="row">
            <ul class="collection with-header">
                <li class="collection-header"><h4>Galaxies</h4></li>
                @foreach (ApplicationRole s in ViewBag.allroles)
                {
                    <li class="collection-item"><a href="#!">@s.Name</a> </li>
                }

            </ul>
           
        </div>
    </div>

</div>
